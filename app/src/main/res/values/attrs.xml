<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TitleBar">
        <attr name="title_text" format="reference|string" />
        <attr name="icon_left_text" format="reference|string" />
        <attr name="icon_right_text" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="MultiWaveHeader">
        <attr name="mwhWaveHeight" format="dimension"/>
        <attr name="mwhStartColor" format="color"/>
        <attr name="mwhCloseColor" format="color"/>
        <attr name="mwhAlphaColor" format="float"/>
        <attr name="mwhWaves" format="string"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>


</resources>
